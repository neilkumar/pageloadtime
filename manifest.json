{
   "name": "Page Load Time",
   "version": "1.0",
   "description": "The first extension that I made.",
   "background_page": "background.html",
   "permissions": [
      "tabs", "http://*/*",
      "http://*/*", 
      "https://*/*"      
   ],
   "browser_action": {
      "default_icon": "icon.png",
      "popup": "popup.html"
   }
}